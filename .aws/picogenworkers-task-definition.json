{
  "family": "pianofi-picogenworkers",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["EC2"],
  "cpu": "2048",
  "memory": "15000",
  "executionRoleArn": "arn:aws:iam::960370336558:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::960370336558:role/pianofi-task-role",
  "containerDefinitions": [
    {
      "name": "picogenworkers",
      "image": "960370336558.dkr.ecr.us-east-1.amazonaws.com/pianofi/picogenworkers:latest",
      "essential": true,
      "resourceRequirements": [
        {"type": "GPU", "value": "1"}
      ],
      "environment": [
        {"name": "AWS_REGION", "value": "us-east-1"},
        {"name": "USE_LOCAL_STORAGE", "value": "false"},
        {"name": "ENVIRONMENT", "value": "production"},
        {"name": "PICOGEN_QUEUE_NAME", "value": "picogen_job_queue_prod"}
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/pianofi-picogenworkers",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs",
          "awslogs-create-group": "true"
        }
      }
    }
  ]
}