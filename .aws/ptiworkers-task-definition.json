{
  "family": "pianofi-ptiworkers",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "2048",
  "memory": "4096",
  "executionRoleArn": "arn:aws:iam::960370336558:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::960370336558:role/pianofi-task-role",
  "containerDefinitions": [{
    "name": "ptiworkers",
    "image": "960370336558.dkr.ecr.us-east-1.amazonaws.com/pianofi/ptiworkers:latest",
    "essential": true,
    "environment": [
      {"name": "AWS_REGION", "value": "us-east-1"},
      {"name": "USE_LOCAL_STORAGE", "value": "false"},
      {"name": "ENVIRONMENT", "value": "production"},
      {"name": "PTI_QUEUE_NAME", "value": "pti_job_queue_prod"}
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/pianofi-ptiworkers",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "ecs",
        "awslogs-create-group": "true"
      }
    }
  }]
}